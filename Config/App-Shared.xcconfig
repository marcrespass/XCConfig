COPYRIGHT_STRING = Copyright Â© $(COPYRIGHT_YEAR) Maine Judicial Branch (OIT). All rights reserved.
GET_INFO_STRING = $(CFBundleVersion) COPYRIGHT_STRING

SWIFT_COMPILATION_MODE = wholemodule

ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES

SWIFT_OBJC_BRIDGING_HEADER = $(PRODUCT_NAME)/$(PRODUCT_NAME)-Bridging-Header.h

